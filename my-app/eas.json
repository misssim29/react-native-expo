{
  "cli": {
    "version": ">= 5.9.3"
  },
  "build": {
    "development": {
      "channel": "development",
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "channel": "preview",
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "android": {
        "buildType": "apk",
        "image": "latest"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://api.socdoc.io/community_v5/prod"
      }
    },
    "production": {}
  },
  "submit": {
    "production": {}
  }
}
